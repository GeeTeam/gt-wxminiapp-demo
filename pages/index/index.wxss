page {
  overflow: hidden;
}
.container {
  width: 100%;
  height: 100%;
  background: linear-gradient(180deg, rgba(21, 90, 255, 0) 0%, #155AFF 100%);
}

.navigator {
  left: 5.3%;
}
.navigator image {
  width: 108px;
  height: 22px;
}

.bg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0px;
  z-index: -99;
}

.c_main {
  padding: 26% 12.8% 0;
  width: 74.4%;
  height: 77.73%;
  display: flex;
flex-direction: column;
justify-content: space-around;

}

.c_main .title {
  text-align: center;
}

.c_main .title view {
  font-size: 14px;
  color: #fff;
  mix-blend-mode: normal;
}

.c_main .title image{
  width: 176px;
  height: 23px;
}
.c_main .title view {
  margin-top: 13px;
  padding: 0 4%;
}


.cutline {
  width: 100%;
  height: 20px;
  position: relative;
}
.cutline .line {
  width: 100%;
  height: 100%;
  position: relative;
}

.cutline .line .left{
  width: 30%;
  height: 1px;
  position: absolute;
  top: 50%;
  background-color: #FFFFFF;
  opacity: 0.4;
  left: -10%;
transform: translateaY(-50%);
border-radius: 6px;
}

.cutline .line .right{
  width: 30%;
  height: 1px;
  position: absolute;
  top: 50%;
  background-color: #FFFFFF;
  opacity: 0.4;
  right: -10%;
transform: translateaY(-50%);
border-radius: 6px;
}

.cutline .line_tips {
  width: 60%;
  height: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
  font-size: 14px;
  color: #FFFFFF;
  background-color: transparent;
  font-weight: 200;
}

.c_main .items {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.c_main .items>view {
  width: 100%;
  height: 60px;
  position: relative;
  background: #FFFFFF;
  box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.1);
  border-radius: 6px;
  margin-bottom: 5.76%;
  line-height: 60px;
  cursor: pointer;
}

.c_main .items view image{
  position: absolute;
  width: 46px;
  height: 46px;
  top: 8px;
  left: 13px;
}
.c_main .items .item-sel {
  width: 100%;
  text-align: center;
  color: #3B426B;
  font-size: 17px;
  cursor: pointer;
}

.footer {
  color: #FFFFFF;
  font-size: 10px;
  opacity: 0.6;
  width: 74.4%;
  padding:0 12.8%;
  text-align: center;
}